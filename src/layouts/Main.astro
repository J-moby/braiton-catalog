---
import Layout from "@layouts/Layout";
import Header from "@/components/Header/Header.astro";
import Footer from "@/components/Footer/Footer.astro";

interface Props {
    title: string;
    headerMod?: string;
}

const { title } = Astro.props;
---

<Layout title={title}>
    <Header />
    <main>
        <slot />
    </main>
    <Footer />
</Layout>
